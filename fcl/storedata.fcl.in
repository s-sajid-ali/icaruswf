process_name : hepnosstore

  outputs : {
      hepnos : {
         module_type: HepnosSPOutput
         fileName: "icarus"
         forwardProducts: true
         async: "${ENABLE_HEPNOS_ASYNC}"
         batch: "${ENABLE_HEPNOS_BATCH}"
      }
  }

services.HepnosDataStore: {
   protocol: "${MERCURY_TRANSPORT_PROTOCOL}"
}

physics : {
 end_path : [hepnos]
 }
